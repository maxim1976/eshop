{
  "providers": ["python"],
  "buildCmd": "python manage.py collectstatic --noinput",
  "startCmd": "gunicorn --bind 0.0.0.0:$PORT --timeout 60 --workers 2 eshop.wsgi:application",
  "variables": {
    "NIXPACKS_PYTHON_VERSION": "3.10",
    "DJANGO_SETTINGS_MODULE": "eshop.settings.production",
    "RAILWAY_ENVIRONMENT": "production"
  }
}